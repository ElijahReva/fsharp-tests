    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:view="clr-namespace:FSharpTest.UI.View"
    xmlns:viewModel="clr-namespace:FSharpTest.UI.ViewModel">

    <DataTemplate DataType="{x:Type viewModel:ArraysViewModel}">
        <view:ArraysView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:TreesViewModel}">
        <view:TreesView />
    </DataTemplate>

</ResourceDictionary>